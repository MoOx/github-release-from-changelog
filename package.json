    "push",
    "github",
    "release",
    "release notes",
    "changelog",
    "history"
  ],
  "author": "Maxime Thirouin",
  "license": "MIT",
  "repository": "https://github.com/MoOx/github-release-from-changelog.git",
  "bin": {
    "github-release-from-changelog": "github-release-from-changelog.js"
  },
  "dependencies": {
